class Solution {
  public:
    double switchCase(int choice, vector<double> &arr) {
        double pi = M_PI;
        switch(choice){
            case 1:
            return pi*arr[0]*arr[0];  // Area of circle
            break;
            case 2:
            return arr[0]*arr[1];  // Area of rectangle
            break;
        }
    }
};
